<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.radrails.rails.ui.railsShellCommandProviders">
      <railsShellCommandProvider
            class="com.aptana.radrails.cloud.shell.AptanaCloudCommandProvider"
            priority="50">
      </railsShellCommandProvider>
   </extension>
   <extension
         point="com.aptana.ide.server.cloud.cloudDeployer">
      <cloudDeployer
            class="com.aptana.radrails.cloud.deploy.RailsCloudDeployer"
            priority="100">
      </cloudDeployer>
   </extension>

</plugin>
